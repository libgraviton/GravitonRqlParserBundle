<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
        http://symfony.com/schema/dic/services/services-1.0.xsd"
        >
    <services>
        <service id="graviton.rql.parser" class="Graviton\Rql\Parser"/>
        <service id="graviton.rql.lexer" class="Graviton\Rql\Lexer"/>

        <!-- Visitors -->
        <service id="graviton.rql.visitor.mongo.odm" class="Graviton\Rql\Visitor\MongoOdm" />

        <!-- Operations -->
        <service id="graviton.rql.operation" class="Graviton\Rql\AST\Operation" />

    </services>
</container>
