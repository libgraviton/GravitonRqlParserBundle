{
    "name": "graviton/rql-parser-bundle",
    "description": "Port of the php-rql-parser into the world of Symfony 2.",
    "license": "GPL",
    "authors": [
        {
            "name": "List of contributors",
            "homepage": "https://github.com/libgraviton/GravitonRqlParserBundle/graphs/contributors"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "non-feature-branches": ["master", "develop", "support/*"],

    "require": {
        "php": ">=5.5",
        "graviton/php-rql-parser": "~2.0@alpha"
    },

    "require-dev": {
        "symfony/framework-bundle": "~2.7",
        "lapistano/proxy-object": "~2.1",
        "doctrine/mongodb-odm": "1.0.0-BETA11@dev",
        "jmikola/geojson": "~1.0",
        "phpunit/phpunit": "~4.7"
    },

    "autoload": {
        "psr-4": {
            "Graviton\\RqlParserBundle\\": ""
        }
    }
}
